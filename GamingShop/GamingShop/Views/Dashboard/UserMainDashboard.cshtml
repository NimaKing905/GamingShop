        <div class="row">
            <div class="col-md-12 stretch-card">
                <div class="card">
                    <div class="card-body">
                        <p class="card-title">فاکتور</p>
                        <div class="table-responsive">
                            <table id="recent-purchases-listing" class="table">
                                <thead>
                                    <tr>
                                        <th>نام خریدار</th>
                                        <th>نام بازی</th>
                                        <th>دستگاه</th>
                                        <th>سبک</th>
                                        <th>تاریخ فروش</th>
                                        <th>قیمت</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        @if (item != null)
                                        {


                                            <tr>
                                                <td>1324</td>
                                                <td>@item.TblAll.Name</td>
                                                <td>@item.TblPlatform.Name</td>
                                                <td>@item.TblCategory.Name</td>
                                                <td>@item.DateTime</td>
                                                <td>@item.Price</td>
                                            </tr>
                                        }
                                        else
                                        {
                                            <tr>فاکتوری وجود ندارد</tr>
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
